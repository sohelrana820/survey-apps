<!-- TOP HEADER AREA START -->
<header class="top-header hidden-xs">
    <div class="container">
        <div class="row">
            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                <div class="clearfix">
                    <ul class="list-inline">
                        <li>
                            <a href="https://join.skype.com/qP01Tf6Xl7cc"><i class="fa fa-skype pr-5 pl-10"></i> Skype</a>
                        </li>
                        <li>
                            <a href="mailto:support@themevessel.com">
                                <i class="fa fa-envelope-o pr-5 pl-10"></i> support@themevessel.com
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
            <div class="col-xs-12 col-sm-6 col-md-6 col-lg-6">
                <div class="clearfix">
                    <div class="top-buttons">
                        <ul class="top-social-media pull-right">
                            <li>
                                <a href="#">Follow Us</a>
                            </li>
                            <li>
                                <a href="https://www.facebook.com/ThemeVessel" class="facebook"><i class="fa fa-facebook"></i></a>
                            </li>
                            <li>
                                <a href="https://twitter.com/theme_vessel" class="twitter"><i class="fa fa-twitter"></i></a>
                            </li>
                            <li>
                                <a href="https://plus.google.com/104339660136115239484" class="google"><i class="fa fa-google-plus"></i></a>
                            </li>
                            <li>
                                <a href="https://www.linkedin.com/company/theme-vessel" class="linkedin"><i class="fa fa-linkedin"></i> </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>
    </div>
</header>
<!-- TOP HEADER AREA END -->

<!-- HEADER AREA START -->
<header class="header">
    <nav class="navbar navbar-default navbar-static-top">
        <div class="container">
            <div class="navbar-header">
                <button type="button" class="navbar-toggle collapsed" data-toggle="collapse" data-target="#navbar" aria-expanded="false" aria-controls="navbar">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </button>
                <a class="navbar-brand theme-color" href="/">Theme<span class="grey-color">Shop</span></a>
            </div>
            <div id="navbar" class="navbar-collapse collapse">
                <ul class="nav navbar-nav">
                    <li class="active"><a href="/">Home</a></li>
                    <li><a href="/products">Products</a></li>
                    <li class="dropdown">
                        <a href="#" class="dropdown-toggle" data-toggle="dropdown" role="button" aria-haspopup="true" aria-expanded="false">
                            Choose Categories <span class="caret"></span>
                        </a>
                        <ul class="dropdown-menu">
                            {% for category in categories %}
                                <li><a href="/products?cat={{ category.slug }}">{{ category.name }}</a></li>
                            {% endfor %}
                        </ul>
                    </li>
                </ul>
                <ul class="nav navbar-nav navbar-right">
                    <li><a href="/faqs">FAQs</a></li>
                    <li><a href="/privacy-policy">Privacy Policy</a></li>
                    <li><a href="/contact-us">Contact Us</a></li>
                </ul>
            </div><!--/.nav-collapse -->
        </div>
    </nav>
</header>
<!-- HEADER AREA END -->

{% include '/elements/flash.twig' %}

{% block banner %}
    <!-- Inject banner content from template file here -->
{% endblock %}

{% block content %}
    <!-- Inject template content here -->
{% endblock %}

<!-- FOOTER AREA START -->
<footer class="footer">
    <div class="container">
        <div class="row">
            <div class="col-ls-3 col-md-3 col-sm-6 footer-widget">
                <h2 class="footer-logo">
                    <a href="/" class="white-color">
                        Theme<span class="grey2-color">Shop</span>
                    </a>
                </h2>
                <p>
                    ThemeVessel Shop is a tiny marketplace to buy Bootstrap templates, HTML5 & CSS3 templates, WordPress Themes
                    UI kits, etc. Purchases your desired web template and theme from our best collection.
                </p>
            </div>

            <div class="col-ls-3 col-md-3 col-sm-6 footer-widget">
                <h2 class="footer-title">Contact Us</h2>
                <address>
                    <ul class="contact-nav">
                        <li>
                            <i class="fa fa-map-marker"></i>
                            <span>
                            Dhaka, Bangladesh
                        </span>
                        </li>
                        <li>
                            <i class="fa fa-envelope"></i>
                            <span>
                            <a href="mailto:support@themevessel.com">support@themevessel.com</a>
                        </span>
                        </li>
                        <li>
                            <i class="fa fa-skype"></i>
                            <span>
                            <a href="https://join.skype.com/qP01Tf6Xl7cc"> https://join.skype.com/qP01Tf6Xl7cc</a>
                        </span>
                        </li>
                        <li>
                            <i class="fa fa-globe"></i>
                            <span>
                            <a href="https://themevessel.com">https://themevessel.com</a>
                        </span>
                        </li>
                    </ul>
                </address>
            </div>
            <div class="col-ls-3 col-md-3 col-sm-6 footer-widget">
                <h2 class="footer-title">Recent Items</h2>
                <div class="footer-items">
                    {% for thumb in recent_products %}
                        <a href="/products/{{ thumb.slug }}">
                            <img src="/{{ thumb.thumb_image }}" alt="{{ thumb.title }}" class="footer-item">
                        </a>
                    {% endfor %}
                </div>
            </div>
            <div class="col-md-3 col-sm-6">
                <h2 class="footer-title">Newsletter</h2>
                <p>
                    Want more templates, themes & UI kits? Subscribe to our mailing list to receive an update when new items published!
                </p>
                <form class="newsletter" id="newsletter">
                    <div class="form-group">
                        <input type="email" name="email" class="form-control" placeholder="Your email address" required>
                    </div>
                    <button type="submit" class="newsletter-submit btn">Subscribe</button>
                </form>
            </div>
        </div>
    </div>
    <footer class="footer-copyright">
        <div class="container">
            <div class="row">
                <div class="col-lg-8 col-sm-8">
                    <p class="copyright">
                        &copy;  2017 <a href="https://themevessel.com/" target="_blank">Theme Vessel</a>. Trademarks and brands are the property of their respective owners.
                    </p>
                </div>
                <div class="col-lg-4 col-sm-4">
                    <ul class="social-links">
                        <li>
                            Follow Us
                        </li>
                        <li>
                            <a class="social-icons" href="https://www.facebook.com/ThemeVessel/">
                                <i class="fa fa-facebook"></i>
                            </a>
                        </li>
                        <li>
                            <a class="social-icons" href="https://twitter.com/theme_vessel">
                                <i class="fa fa-twitter"></i>
                            </a>
                        </li>
                        <li>
                            <a class="social-icons" href="https://plus.google.com/104339660136115239484">
                                <i class="fa fa-google-plus"></i>
                            </a>
                        </li>
                        <li>
                            <a class="social-icons" href="https://www.linkedin.com/company/theme-vessel/">
                                <i class="fa fa-linkedin"></i>
                            </a>
                        </li>
                    </ul>
                </div>
            </div>
        </div>
    </footer>
</footer>
<!-- FOOTER AREA END -->

